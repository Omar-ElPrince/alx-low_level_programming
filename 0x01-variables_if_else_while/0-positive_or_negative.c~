#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/*
* main - entry
*
* Description: THe function is false or true
*
* Return: 0 (correct)
*/
int main(void)
{
int n;

srand(time(0));
n = rand() - RAND_MAX / 2;
if (n > 0)
printf("%i is positive\n" ,n);
else if (n == 0)
printf("%i is Zero\n" ,n);
else (n < 0)
printf("%i is negative\n" ,n);
return (0);
}
